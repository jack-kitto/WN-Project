// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Direction {
  Entering
  Leaving
}

model area1 {
  id Int @id @default(autoincrement())
  time Int 
  direction Direction
}

model area2 {
  id Int @id @default(autoincrement())
  time Int 
  direction Direction
}

model area3 {
  id Int @id @default(autoincrement())
  time Int 
  direction Direction
}